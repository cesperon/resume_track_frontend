\"@/components/applicationForm.vue\"\n\n  export default {    \n      name: \"Dashboard\",\n      components: {\n        appForm,\n\n      },    \n      data() {    \n          return {    \n              user: null,\n              token: null,\n              popUp: false,\n              userApps: null, \n          }    \n      },    \n      methods: {    \n          getUserData: function() {    \n            this.user = store.getters['userDetails'];\n            this.token = store.getters[\"userToken\"];\n          },\n          async getApplications(){\n            try{\n              const res = await axios.get(\"/applications\", {\n                headers: {\n                  Authorization: `Bearer ${this.token}`,\n                  },\n                });\n              console.log(\"data\", res.data.data);\n              store.commit(\"setUserApps\", res.data.data);\n              this.userApps = store.getters[\"userApps\"];\n              console.log(\"userapps\", this.userApps);\n            }\n            catch(e){\n              console.log(e);\n            }\n \n          },\n\n          openModal() {\n            this.popUp = !this.popUp;\n          },\n\n          logOut() {\n            store.commit(\"clearToken\");\n            this.$router.push('/');\n          },\n        },    \n      mounted() {    \n          this.getUserData();\n          this.getApplications();\n      }    \n  }\n</script>"]}]}