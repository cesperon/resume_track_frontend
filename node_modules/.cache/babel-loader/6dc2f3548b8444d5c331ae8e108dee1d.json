AAA,CAAA,OAAA,CAAA,UAAA,CAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,UAAA,EAAA,KAAA,CAAA,QAAA;AAVA;AAAA;;AAAA;AAAA;AAAA;AAaA,gBAAA,OAAA,CAAA,GAAA;;AAbA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,KArBA;AAuBA,IAAA,SAvBA,uBAuBA;AACA,WAAA,KAAA,GAAA,CAAA,KAAA,KAAA;AACA,KAzBA;AA2BA,IAAA,MA3BA,oBA2BA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,YAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AA9BA,GAdA;AA8CA,EAAA,OA9CA,qBA8CA;AACA,SAAA,WAAA;AACA,SAAA,eAAA;AACA;AAjDA,CAAA","sourcesContent":["<template>\n  <div class=\"about\">\n    <h1>Dashboard {{user.firstName}} @ {{user.email}}</h1>\n    <button v-on:click=\"openModal\">new application</button>\n    <appForm v-if=\"popUp\" @clicked=\"openModal\"></appForm>\n    <button v-on:click=\"logOut\">Logout</button>\n    <div v-if=\"userApps\">\n      <ul v-for=\"apps in userApps\" :key=\"apps.company_name\">\n        <div>\n          {{apps.companyName}},\n          {{apps.position}},\n          {{apps.location}},\n          {{apps.dateApplied}},\n        </div>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\n  import axios from \"axios\"    \n  import router from \"../router\" \n  import store from \"@/store\"\n  import appForm from \"@/components/applicationForm.vue\"\n\n  export default {    \n      name: \"Dashboard\",\n      components: {\n        appForm,\n\n      },    \n      data() {    \n          return {    \n              user: null,\n              token: null,\n              popUp: false,\n              userApps: null, \n          }    \n      },    \n      methods: {    \n          getUserData: function() {    \n            this.user = store.getters['userDetails'];\n            this.token = store.getters[\"userToken\"];\n          },\n          async getApplications(){\n            try{\n              const res = await axios.get(\"/applications\", {\n                headers: {\n                  Authorization: `Bearer ${this.token}`,\n                  },\n                });\n              console.log(\"data\", res.data.data);\n              store.commit(\"setUserApps\", res.data.data);\n              this.userApps = store.getters[\"userApps\"];\n              console.log(\"userapps\", this.userApps);\n            }\n            catch(e){\n              console.log(e);\n            }\n \n          },\n\n          openModal() {\n            this.popUp = !this.popUp;\n          },\n\n          logOut() {\n            store.commit(\"clearToken\");\n            this.$router.push('/');\n          },\n        },    \n      mounted() {    \n          this.getUserData();\n          this.getApplications();\n      }    \n  }\n</script>"],"sourceRoot":"src/views"}]}